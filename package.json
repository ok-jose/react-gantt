{
  "name": "react-gantt-svg",
  "private": false,
  "version": "1.0.2",
  "description": "A React Gantt chart component library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:types && npm run build:lib",
    "build:types": "tsc -p tsconfig.lib.json",
    "build:lib": "vite build --config vite.lib.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "gantt",
    "chart",
    "component",
    "typescript"
  ],
  "author": "jose",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ok-jose/react-gantt.git"
  },
  "homepage": "https://github.com/ok-jose/react-gantt#readme",
  "bugs": {
    "url": "https://github.com/ok-jose/react-gantt/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "@dnd-kit/core": "^6.0.0",
    "@dnd-kit/modifiers": "^9.0.0",
    "debug": "^4.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@eslint/js": "^9.33.0",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-onboarding": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "debug": "^4.4.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.2",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^9.1.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}